[Unit]
Description=CLIProxyAPIPlus Proxy Server
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/cli-proxy/CLIProxyAPIPlus
ExecStart=/home/ubuntu/cli-proxy/CLIProxyAPIPlus/cli-proxy-api-plus
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
